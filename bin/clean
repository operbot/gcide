#!/usr/bin/env python3
# This file is placed in the Public Domain.


"clean gcid"


import os


def popen(txt):
    for line in os.popen(txt).readlines():
        print(line)


popen("rm -fR txt/*~ files/*~ files/__pycache__")
popen("rm -fR bin/*~ *~ __pycache__")
popen("rm -fR build dist MANIFEST gcide.egg-info")
popen("rm -fR files/*~")
popen("rm -fR gcide/*~ gcide/__pycache__")
popen("rm -fR .test test/__pycache__ test/*~")
popen("rm -fR docs/*~ docs/_static/*~ docs/_templates/*~")
popen("rm -fR docs/gcide*.rst")
popen("rm -fR html")
